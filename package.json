{
  "name": "the-best-thing",
  "version": "1.0.0",
  "description": "A fun meme website where users compare two things and vote for the better one",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "seed": "node server/scripts/seed-wikipedia.js",
    "seed:large": "node server/scripts/seed-wikipedia.js 500",
    "seed:popular": "node server/scripts/seed-popular.js"
  },
  "keywords": [
    "meme",
    "comparison",
    "ranking",
    "wikipedia"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
